    "@tailwindcss/vite": "^4.1.11",
    "antd": "^6.1.0",
    "axios": "^1.7.9",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "effector": "^23.4.4",
    "effector-react": "^23.3.0",
    "formik": "^2.4.9",
    "i18next": "^25.8.1",
    "jwt-decode": "^4.0.0",
    "lodash-es": "^4.17.21",
    "oidc-client-ts": "^3.1.0",
    "patronum": "^2.3.0",
    "rc-virtual-list": "^3.19.2",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-i18next": "^16.5.4",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.9.6",
    "react-transition-group": "^4.4.5",
    "tailwindcss": "^4.1.11",
    "uuid": "^13.0.0",
    "zod-formik-adapter": "^2.0.0"
