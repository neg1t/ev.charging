    "vite": "^7.0.4",
    "vite-plugin-version-mark": "^0.2.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "NODE_OPTIONS=--max_old_space_size=8192 lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint",
      "prettier --write",
      "bash -c 'tsc --noEmit'",
      "bash -c 'steiger ./src'"
    ]
  },
  "packageManager": "yarn@4.12.0"
}
