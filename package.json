{
  "name": "sf.ev.charging.admin",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "cross-env VITE_VERSION=$npm_package_version vite --host",
    "build": "cross-env tsc && vite build",
    "lint": "eslint .",
    "ts:check": "tsc --noEmit",
    "fsd:check": "steiger ./src",
    "test": "vitest run",
    "preview": "vite preview",
    "generate": "kubb generate",
    "format": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\"",
    "prepare": "husky"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
